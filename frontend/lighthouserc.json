{
  "ci": {
    "collect": {
      "url": [
        "http://127.0.0.1:4173/",
        "http://127.0.0.1:4173/projects",
        "http://127.0.0.1:4173/contact"
      ],
      "startServerCommand": "pnpm --filter @personal-website/public preview -- --host 127.0.0.1 --port 4173",
      "startServerReadyPattern": "Local",
      "numberOfRuns": 1
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "categories:performance": [
          "error",
          {
            "minScore": 0.9
          }
        ]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
